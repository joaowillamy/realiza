{
  "name": "shared-types",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/shared/types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/modules/shared/types",
        "main": "modules/shared/types/src/index.ts",
        "tsConfig": "modules/shared/types/tsconfig.lib.json",
        "assets": ["modules/shared/types/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["modules/shared/types/**/*.ts"]
      }
    }
  },
  "tags": []
}
